    "inpainter": {
        "epochs": 20000,
        "masking_level": 40,
        "lamb": 0.01,
        "save_dir": "gann_4_night_20210905_40/",
        "save_period": 100,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 20000,
        "tensorboard": true
    }
