    "loss": "inpaint_loss",
    "metrics": [],
    "trainer": {
        "epochs": 300,
        "save_dir": "gann_4_night_20210905_40/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": [
            "off",
            "off"
