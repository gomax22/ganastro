{
    "name": "GANASTRO",
    "n_gpu": 4,
    "generator": {
        "type": "Generator",
        "args": {
            "num_features": 8,
            "latent_dim": 512,
            "num_channels": 1,
            "n_layers": 6
        }
    },
    "discriminator": {
        "type": "DiscriminatorDropout",
        "args": {
            "num_features": 8,
            "num_channels": 1,
            "n_dropout": 3,
            "n_layers": 6
        }
    },
    "data_loader": {
        "type": "NpzDataLoader",
